{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "plugins": ["eslint", "import", "jsdoc", "oxc", "promise", "typescript", "typescript", "unicorn"],
  "categories": {
    "correctness": "error",
    "perf": "error",
    "style": "error",
    "nursery": "warn",
    "pedantic": "warn",
    "restriction": "warn",
    "suspicious": "warn"
  },
  "rules": {
    "id-length": "allow",
    "no-magic-numbers": ["error", { "ignoreTypeIndexes": true }],
    "no-named-export": "allow"
  },
  "settings": {},
  "env": {
    "builtin": true
  },
  "globals": {},
  "ignorePatterns": []
}
